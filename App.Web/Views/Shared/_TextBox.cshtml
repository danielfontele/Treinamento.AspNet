
@model TextBoxModel


<input type="text" 
       name="@(Model.Propriedade)" 
       id="@(Model.Propriedade)" 
       value = "@(Model.Valor)"
       placeholder = "@(Model.Placeholder)"
       @if (!Model.Habilite) { <text>disabled</text>}
       
       />

<script>
     $(function(){
            app.carregueJavascript("/js/app.textbox.js", {
                callback: function() {
                    new app.textbox($("#@(Model.Propriedade)"));
                }
            });
        });
</script>